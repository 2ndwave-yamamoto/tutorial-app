<h1>新規登録</h1>
<div class="login">
  <%= form_with model: @user do |form| %>
    <%= form.text_field :name, placeholder: "ユーザ名" %>
    <%= form.password_field :password , placeholder: "パスワード"%>
    <%= form.submit "登録", class: "btn btn-primary" %>
  <% end %>
</div>
<div class="back">
  <%= link_to t('back'), root_path, class: "btn btn-primary" %>
</div>